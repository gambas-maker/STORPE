<h1>Listing all articles</h1>
<table>
 <tr>
  <th>Title</th>
  <th>Description</th>
 </tr>
 <% @articles.each do |article| %>
 <tr>
  <td><%= cl_image_tag article.photo.key, height: 300, width: 400, crop: :fill %> </td>
  <td><%= article.title %></td>
  <td><%= article.description %></td>
  <td><%= link_to 'Show', article_path(article) %></td>
 </tr>
 <% end %>
</table>

<div align="center">
 <%= will_paginate %>
</div>

<%=  render "shared/navbarbottom" %>
